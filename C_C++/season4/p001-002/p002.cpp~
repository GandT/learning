#include <iostream>
#include <queue>

using namespace std;

int main()
{
  //priority queue declaration
  priority_queue<int> pq;

  //input numbers and set(enqueue) numbers to priority queue
  int in[5];
  for(int i=0;i<5;++i){
    cin >> in[i];
    pq.push(in[i]);
  }

  //print a line
  cout << "----------" << endl;

  //pop(dequeue) numbers from priority queue
  for(int i=0;i<5;++i){
    cout << pq.top() << endl;
    pq.pop();
  }

  //return
  return 0;
}
